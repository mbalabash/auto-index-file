{
  "name": "auto-index-file",
  "version": "0.1.4",
  "description": "Index.js file without tears",
  "scripts": {
    "eslint": "eslint ./src --cache --fix",
    "test": "ava"
  },
  "dependencies": {
    "acorn-loose": "^6.0.0",
    "argparse": "^1.0.10",
    "chalk": "^2.4.2",
    "chokidar": "^2.0.4"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "autoIndexFile": "./index.js"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/__mock__/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js)": [
      "yarn eslint",
      "git add"
    ]
  },
  "keywords": [
    "index.js",
    "js",
    "components",
    "atomic design"
  ],
  "author": "mbalabash",
  "license": "MIT"
}
