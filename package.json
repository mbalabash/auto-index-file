{
  "name": "auto-index-file",
  "version": "0.1.9",
  "description": "Index.js file without your tears ðŸ˜‰",
  "scripts": {
    "eslint": "eslint ./src --cache --fix",
    "test": "ava"
  },
  "dependencies": {
    "acorn-loose": "^7.0.0",
    "argparse": "^1.0.10",
    "chalk": "^2.4.2",
    "chokidar": "^3.0.2"
  },
  "devDependencies": {
    "ava": "^2.3.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2"
  },
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "autoIndexFile": "./index.js"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/__mock__/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js)": [
      "yarn eslint",
      "git add"
    ]
  },
  "keywords": [
    "index.js",
    "js",
    "components",
    "atomic design"
  ],
  "homepage": "https://github.com/mbalabash/auto-index-file/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/mbalabash/auto-index-file"
  },
  "author": "mbalabash",
  "license": "MIT"
}
